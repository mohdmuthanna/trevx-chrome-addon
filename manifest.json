{
  "name": "Trevx",
  "version": "0.2",
  "description": "play & pause, view list of audio links",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["popup.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_title": "communicate with background",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"

  ],
  "manifest_version": 2
}
