{
  "name": "Trevx",
  "version": "0.2",
  "description": "Welcome to Trevx, The leading audio search engine in the world, The place you can find, play and download any audio file on the web",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["popup.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_title": "communicate with background",
    "default_popup": "popup.html",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16" : "64.png",
    "48" : "64.png",
    "64" : "64.png",
    "128" : "64.png"
  },
  
  "permissions": [
    "activeTab",
    "storage"

  ],
  "manifest_version": 2
}
