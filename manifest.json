{
  "name": "Trevx Music Player",
  "version": "0.1.1",
  "version_name": "0.1.1",
  "description": "Trevx Music Player; allow you to listen to music while browsing",
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_title": "",
    "default_popup": "popup.html",
    "default_icon": "images/welcome.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16" : "images/64.png",
    "48" : "images/64.png",
    "64" : "images/64.png",
    "128" : "images/64.png"
  },
  "homepage_url": "http://trevx.com/",
  "permissions": [
    "activeTab",
    "*://trevx.com/*",
    "storage",
    "webRequest"


  ],
  "manifest_version": 2
}
