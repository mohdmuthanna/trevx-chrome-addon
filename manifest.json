{
  "name": "Trevx",
  "version": "0.1",
  "version_name": "0.1 beta",
  "description": "Trevx is audio search engine, The place you can find, play and download any audio file on the web",
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_title": "",
    "default_popup": "popup.html",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16" : "images/64.png",
    "48" : "images/64.png",
    "64" : "images/64.png",
    "128" : "images/64.png"
  },
  "homepage_url": "http://trevx.com/",
  "permissions": [
    "activeTab",
    "storage"

  ],
  "manifest_version": 2
}
